{
  "name": "muhammadhanif/codeigniter-bmkg-gempa",
  "type": "project",
  "description": "Data gempabumi Indonesia: gempabumi terkini, gempabumi magnitude 5.0+, & gempabumi dirasakan.",
  "keywords": [
    "ci4",
    "codeigniter4",
    "codeigniter",
    "gempa",
    "gempabumi",
    "bmkg",
    "bmkg-api",
    "bmkg-json"
  ],
  "homepage": "https://github.com/muhammadhanif/codeigniter-bmkg-gempa",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "kint-php/kint": "^3.3",
    "laminas/laminas-escaper": "^2.6",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "codeigniter4/codeigniter4-standard": "^1.0",
    "fzaninotto/faker": "^1.9@dev",
    "mikey179/vfsstream": "1.6.*",
    "phpunit/phpunit": "^8.5",
    "predis/predis": "^1.1",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "CodeIgniter\\": "system/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "@composer dump-autoload",
      "CodeIgniter\\ComposerScripts::postUpdate"
    ],
    "test": "phpunit"
  },
  "support": {
    "chat": "https://t.me/muhammad_hanif",
    "source": "https://github.com/muhammadhanif/codeigniter-bmkg-gempa"
  }
}
